{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "2sxc-dnn742/Website/DesktopModules/**.csproj",
            "eav-server/**.csproj"
          ],
          "cwd": "c:/projects/",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/*Testing/**"
          ]
        }
      ],
      "dest": "obj/api/dot-net/",
      "filter": "filterConfig.yml",
      "force": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/**.md",
          "how-to/**.md",
          "specs/**.md",
          "specs/**.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "/assets/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "xrefmap.yml"
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
          "namespaces/**.md",
          "shared/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "c:/Projects/2sxc-docs/docs",
    "template": [
      "default",
      "statictoc",
      "material/material",
      "templates/2sxc"
    ],
    "globalMetadata": {
      "_appTitle": "2sxc and EAV 10",

      "_appLogoPath": "images/logos/2sxc-10-50.png",
      "_appFaviconPath": "images/logos/favicon.ico",
      "_enableNewTab": true,
      "_enableSearch": true
    }
  }
}
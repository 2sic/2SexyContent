{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "2sxc/**.csproj",
            "eav-server/**.csproj"
          ],
          "cwd": "../../..",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**",
            "**/*Testing/**",
            "**/*.Tests/**",
            "**/XOLD-*/**",
            "**Mvc/Website/**",
            "**/ToSic.Sxc.Mvc/**",
            "**/Oqtane/**"

          ]
        }
      ],
      "dest": "obj/api/dot-net/",
      "filter": "filterConfig.yml",
      "force": true,
      "properties": {
        "TargetFramework": "net451"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/**.md",
          "how-to/**.md",
          "specs/**.md",
          "specs/**.yml",
          "how-to/**.yml",
          "toc.yml",
          "*.md",
          "api-meta.json"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "/assets/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "xref": [
      "xrefmap.yml"
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
          "namespaces/**.md",
          "shared/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../../../2sxc-docs/docs",
    "template": [
      "default",
      "statictoc",
      "material/material",
      "templates/2sxc"
    ],
    "globalMetadata": {
      "_appTitle": "2sxc and EAV 10",

      "_appLogoPath": "images/logos/v11/2sxc50.png",
      "_appFaviconPath": "images/logos/v11/favicon.ico",
      "_enableNewTab": true,
      "_enableSearch": true
    }
  }
}